<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <cn.walkpast.rahmen.RahmenView
        android:id="@+id/rahmen_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:rhv_backgroud="@drawable/bg_rahmen2"
        app:rhv_duration="1000"
        app:rhv_image_height="@dimen/rhv_pw_height"
        app:rhv_image_rotation="-10"
        app:rhv_image_width="@dimen/rhv_pw_width"
        app:rhv_image_x="10%"
        app:rhv_image_y="10%"
        />
